#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../nqm-csv-import/nqm-csv-import.js" "$@"
  ret=$?
else 
  node  "$basedir/../nqm-csv-import/nqm-csv-import.js" "$@"
  ret=$?
fi
exit $ret
